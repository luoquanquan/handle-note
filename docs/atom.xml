  <entry>
    <title>HTTP 实践</title>
    <link href="http://note.niubishanshan.top/%E5%89%8D%E7%AB%AF/%E5%AD%A6%E4%B9%A0%E7%AC%94%E8%AE%B0/HTTP/HTTP%20%E5%AE%9E%E8%B7%B5/"/>
